{
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [ "error", 2 ],
        "quotes": [ "error", "single" ],
        "semi": [ "error", "always" ],
        "no-console": "off"
    }
}